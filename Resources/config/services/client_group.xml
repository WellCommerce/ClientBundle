<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <imports>
        <import resource="admin/client_group.xml"/>
    </imports>

    <services>
        <service id="client_group.manager" class="WellCommerce\Bundle\ClientBundle\Manager\ClientGroupManager" parent="core.manager">
            <argument type="service" id="client_group.repository"/>
            <argument type="service" id="client_group.factory"/>
        </service>
    </services>
</container>
